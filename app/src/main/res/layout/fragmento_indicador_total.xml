                    android:layout_toEndOf="@+id/txtmsnreproductores_rt"
                    android:layout_toRightOf="@+id/txtmsnreproductores_rt"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:textAllCaps="true"
